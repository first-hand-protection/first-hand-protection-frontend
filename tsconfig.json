{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "types": ["vite/client"],
    "outDir": "../.cache/typescript-app",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "files": [],
  "references": [
    { "path": "./app" },
    { "path": "./db" },
    { "path": "./edge" },
    { "path": "./scripts" },
    { "path": "./server" }
  ]
}
